{
  "manifest_version": 2,
  "name": "TuLong",
  "version": "1.4.2",
  "description": "Tracks time spent on each tab and saves it daily.",
  "permissions": ["tabs", "storage"],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "48": "icons/icon-48.png",
      "96": "icons/icon-96.png"
    }
  }
}
